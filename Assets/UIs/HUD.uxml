            <engine:VisualElement name="VisualElement" class="flavor-bar-container">
                <engine:ProgressBar value="70" title="sweet" name="sweetProgress" class="flavorBar sweet">
                    <Bindings>
                        <engine:DataBinding property="value" data-source-path="sweetPercent.value" data-source="project://database/Assets/Scripts/Flavors/PlayerFlavorInstance.asset?fileID=11400000&amp;guid=061e8c4953765bb48aa368fae578913d&amp;type=2#PlayerFlavorInstance" binding-mode="ToTarget" />
                    </Bindings>
                </engine:ProgressBar>
                <engine:VisualElement name="VisualElement" class="target-container">
                    <engine:VisualElement name="VisualElement" class="target-marker" style="margin-left: 0;">
                        <Bindings>
                            <engine:DataBinding property="style.marginLeft" data-source-path="sweetPercent" data-source="project://database/Assets/Scripts/UI/TargetFlavor.asset?fileID=11400000&amp;guid=4dabeedb2842e6f41842796a6a636ddc&amp;type=2#TargetFlavor" binding-mode="ToTarget" />
